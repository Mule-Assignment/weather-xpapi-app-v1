#%RAML 1.0 DataType

type: object
properties:
  highTemperature: 
    type: string
    required: true
    example: "10+F"
  lowTemperature: 
    type: string
    required: true
    example: "10+F"
  currentTemperature: 
    type: string
    required: true
    example: "10+F"
  currentOutlook: 
    type: string
    required: true
    enum:
      - sunny
      - raining
      - moderate
    example: "sunny"
